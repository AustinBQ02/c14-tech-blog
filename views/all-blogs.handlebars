<div class="blogs">
  {{#each blogs}}
  <article class="card">
    <div class="card-header flex-row justify-space-between">
      <h2>
        <a href="/blog/{{id}}">{{title}}</a>
      </h2>
      <div>
        Posted by {{user.username}} on {{format_date date_created}}
      </div>
    </div>
    <div class="card-body">
      {{content}}
    </div>
  </article>
  {{/each}}
</div>